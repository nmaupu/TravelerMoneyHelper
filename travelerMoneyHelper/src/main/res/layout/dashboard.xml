<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="horizontal" >

    <org.maupu.android.tmh.ui.widget.DashboardLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/dashboard_button_operations"
            style="@style/DashboardButton"
            android:drawableTop="@drawable/dashboard_operation_button"
            android:text="@string/dashboard_operation" />

        <Button
            android:id="@+id/dashboard_button_stats"
            style="@style/DashboardButton"
            android:drawableTop="@drawable/dashboard_stats_button"
            android:text="@string/dashboard_stats" />
    </org.maupu.android.tmh.ui.widget.DashboardLayout>

</LinearLayout>